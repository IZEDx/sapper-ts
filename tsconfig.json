{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "allowUnreachableCode": false,
      "declaration": false,  
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "lib": [
        "dom",
        "esnext"
      ],
      "moduleResolution": "node",
      "module": "esnext",
      "target": "es2015",
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "strict": true,
      "types": ["@pyoner/svelte-types"]
    },
    "exclude": [
      "node_modules"
    ]
}